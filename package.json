{
  "name": "@geins/ralph-module-streamify",
  "version": "1.0.4",
  "description": "Streamify module for Geins PWA Storefront Ralph",
  "author": "arvidsson-geins",
  "keywords": [
    "vue",
    "nuxt",
    "nuxt.js"
  ],
  "main": "lib/module.js",
  "license": "MIT",
  "scripts": {
    "dev": "nuxt test/fixture",
    "test": "jest",
    "publish": "npm publish --access public",
    "test:unit": "jest --testRegex \"/test/unit/(.+)\\.test\\.js$\"",
    "test:system": "jest --testRegex \"/test/system/(.+)\\.test\\.js$\"",
    "test:coverage": "yarn test --coverage",
    "format": "prettier './**/*.{js,ts,json,vue}' --write"
  },
  "devDependencies": {
    "jest": "^23.4.1",
    "nuxt": "^2.4.0",
    "prettier": "^1.16.1",
    "puppeteer": "^1.6.1"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "lib/**/*.{js,jsx}"
    ]
  },
  "dependencies": {},
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geins-io/ralph-module-streamify.git"
  },
  "bugs": {
    "url": "https://github.com/geins-io/ralph-module-streamify/issues"
  },
  "homepage": "https://github.com/geins-io/ralph-module-streamify#readme"
}
